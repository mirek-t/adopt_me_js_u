{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommeded",
    "plugin:jsx-a11y/recommeded",
    "prettier"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "parseOptions": {
    "ecmaVersion": 2022,
    "sourseType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "versions": "detect"
    }
  }
}
